.container {
  width: 100%;
  height: 100%;
  display: flex;
  padding: 0px;
  flex-direction: column;
}

.searchContain{
  width: 95%;
  position: relative;
  margin-left: 20rpx;
  margin-right: 20rpx;
  margin-top: 30rpx;
  height: 50rpx;
}
.searchInput{
  background: white;
  height: 70rpx;
  font-size: 25rpx;
  border: 1px solid rgba(73, 191, 141, 1);
  border-radius: 35rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  color: #717171;
}
.searchIcon{
  position: absolute;
  right: 20rpx;
  height: 60rpx;
  top: 10rpx;
}
.resultContain{
  margin-top: 30rpx;
  width: 90%;
}
.resultItem{
  width: 100%;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  text-align: center;
  justify-content: space-between;
}
.resultItemText{
  color: rgba(0, 0, 0, 1);
  font-size: 14px;
  height: 80rpx;
  line-height: 80rpx;
}
.tagView{
  height: 50rpx;
  margin-top: 15rpx;
  line-height: 50rpx;
  border: 1px solid rgba(255, 255, 255, 0);
  border-radius: 15px;
  background-color: rgba(242, 242, 242, 1);
  color: rgba(61, 61, 61, 1);
  font-size: 12px;
  text-align: center;
  padding-left: 40rpx;
  padding-right: 40rpx;
}
.line{
  width: 100%;
  height: 1rpx;
  background-color:  rgba(247, 247, 247, 1);
}