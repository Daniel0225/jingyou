.container {
  width: 100%;
  height: 100%;
  display: flex;
  padding: 0px;
  flex-direction: column;
}

.item {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 160rpx;
  align-items: center;
}

.header {
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
  margin-left: 40rpx;
}

.itemText {
  position: absolute;
  right: 90rpx;
  color: rgba(153, 153, 153, 1);
  font-size: 25rpx;
  text-align: left;
  font-family: PingFangSC-regular;
}
.moreImage{
  position: absolute;
  right: 30rpx;
  width: 30rpx;
  height: 40rpx;
}
.line{
  width: 100%;
  height: 1rpx;
  background-color: #ECECEC;
}

